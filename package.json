{
  "name": "react-consent-shield",
  "version": "0.9.2",
  "description": "A comprehensive React library for GDPR, CCPA, and LGPD cookie consent management with real script blocking, geo-detection, and Google Consent Mode v2 support",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.umd.js",
  "jsdelivr": "./dist/index.umd.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./umd": "./dist/index.umd.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.2.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitest/coverage-v8": "^4.0.18",
    "jsdom": "^24.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.3.0"
  },
  "keywords": [
    "react",
    "gdpr",
    "ccpa",
    "lgpd",
    "cookie-consent",
    "consent-management",
    "privacy",
    "cookies",
    "tracking",
    "google-consent-mode",
    "script-blocking",
    "compliance"
  ],
  "author": "686f6c61",
  "license": "PolyForm-Noncommercial-1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/686f6c61/react-consent-shield.git"
  },
  "bugs": {
    "url": "https://github.com/686f6c61/react-consent-shield/issues"
  },
  "homepage": "https://react-consent-shield.onrender.com",
  "engines": {
    "node": ">=18.0.0"
  }
}
